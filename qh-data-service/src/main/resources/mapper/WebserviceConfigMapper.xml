<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.asiainfo.qh.qhdataservice.mapper.WebserviceConfigMapper">

    <!-- 查询接口配置数据 -->
    <select id="queryWebserviceConfig" parameterType="map" resultType="com.asiainfo.qh.qhdataservice.bean.WebserviceConfigBean">
        select webservice_code,webservice_query_sql from nmk.webservice_config_sql where webservice_code=#{webservice_code}
    </select>

</mapper>